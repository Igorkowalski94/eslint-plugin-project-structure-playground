{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "removeComments": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@stories/*": ["examples/enforceExistenceExample/stories/*"]
    },
    "allowJs": true
  },
  "include": [
    "src",
    "./eslint.config.mjs",
    "./folderStructure.mjs",
    "./independentModules.mjs",
    "./namingRules.mjs"
  ]
}
