{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "removeComments": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@stories/*": ["examples/enforceExistenceExample/stories/*"],
      "fixedAlias": [
        "examples/enforceExistenceExample/stories/component.stories.tsx"
      ]
    },
    "allowJs": true
  },
  "include": [
    "src",
    "./eslint.config.mjs",
    "./folderStructure.mjs",
    "./independentModules.mjs",
    "./fileComposition.mjs"
  ]
}
